<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_section1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1.AIDL初步使用" />

        <Button
            android:id="@+id/btn_section2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="2.最基本的Binder接口" />

        <Button
            android:id="@+id/btn_section3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="3.手写AIDL" />

        <Button
            android:id="@+id/btn_section4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4.手写ServiceManager(SM)" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>